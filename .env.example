# GitHub Configuration (Required)
GITHUB_PERSONAL_ACCESS_TOKEN=ghp_your_token_here
GITHUB_REPOSITORY=owner/repository

# dbt File Patterns (Required)
DBT_SCHEMA_PATTERNS=models/**/*.yml
# For multiple patterns use comma separation:
# DBT_SCHEMA_PATTERNS=models/marts/**/*.yml,models/staging/**/*.yml

# Path to dbt_project.yml (Optional, default: dbt_project.yml)
DBT_PROJECT_PATH=dbt_project.yml

# Cache Configuration (Optional)
CACHE_TTL_MINUTES=60
CACHE_SIZE=1000

# Server Configuration (Optional)
LOG_LEVEL=INFO